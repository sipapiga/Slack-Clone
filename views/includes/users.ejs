<div class="row my-5 justify-content-center">
  <div class="row my-4">
    <h3>
      Users
    </h3>
  </div>
  <div class="container">
    <ul class="list-unstyled">
      <div class="text-center well">
        <% if(users.length > 0) { %> <%for(user of users) {%>
        <li><a href="/DM/<%=user._id%>"><%=user.name%><span> <i class="fas fa-circle"></i></span></a></li>
        <%}%> <% } %>
      </div>
    </ul>
  </div>
</div>